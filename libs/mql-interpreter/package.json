    "check-warning-docs": "npm run build && node scripts/updateWarningDocs.js --check",
    "update-builtin-docs": "npm run build && node scripts/updateBuiltinDocs.js",
    "check-builtin-docs": "npm run build && node scripts/updateBuiltinDocs.js --check"
  },
