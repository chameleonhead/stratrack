{
  "name": "mql-interpreter",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "bin": {
    "mql-interpreter": "./bin/mql-interpreter.js"
  },
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint .",
    "test": "npm run check-warning-docs && npm run check-builtin-docs && vitest run --coverage",
